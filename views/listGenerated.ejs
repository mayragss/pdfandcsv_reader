<!DOCTYPE html>
<html lang="pt">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link href="/css/listGenerated.css" rel="stylesheet">
    <title>Formulários Gerados</title>
</head>
<body>
    <p>Formulários gerados a partir do csv</p>

<div class="list">
  <ul>
    <li>Nome</li>
    <li>Idade</li>
    <li>Email</li>
    <li>Instituição</li>
    <li>Curso</li>
    <li>Status</li>
    <li>Link para acesso</li>
    <li>Enviar Email</li>
  </ul>
  <% forms.forEach(function(form,i){ %>   
    <ul>
      <li data-label="Nome"><%=form.Nome%></li>
      <li data-label="Idade"><%=form.Idade%></li>
      <li data-label="Email"><%=form.Email%></li>
      <li data-label="Instituicao"><%=form.Instituicao%></li>
      <li data-label="reference"><%=form.Curso%></li>
      <li data-label="Status">
        <p><%=form.Status%></p>
      </li>
      <li data-label="number"><a href="<%=form.Link%>" target="_blank"> Abrir o Formulário</a> </li>
      <li data-label="number"><a href="/form/send/<%=form.Guid%>" target="_blank"> Enviar</a> </li>
    </ul>
    <%});%>
 
</div>
</body>
</html>